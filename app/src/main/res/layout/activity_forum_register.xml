<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.alpha.alphaapp.ui.widget.TitleLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:txt_title="@string/input_forum_nick_name" />

    <com.alpha.alphaapp.ui.widget.et.AccountEditText
        android:id="@+id/activity_forum_register_aet_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:hint="@string/input_forum_nick_name" />

    <Button
        android:id="@+id/activity_forum_register_btn_insure"
        style="@style/btn_one_com_red"
        android:layout_marginTop="@dimen/commom_margin_10"
        android:text="@string/insure" />
</LinearLayout>