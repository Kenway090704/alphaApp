<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.alpha.alphaapp.ui.widget.TitleLayout
        android:id="@+id/score_exchange_enter_titlelayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:txt_title="@string/my_score" />



   <com.alpha.alphaapp.ui.widget.mall.ScoreExchangeEnterItem
       android:id="@+id/score_exchange_enter_msi_tranform_car"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"/>
    <com.alpha.alphaapp.ui.widget.LineView
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
    <com.alpha.alphaapp.ui.widget.mall.ScoreExchangeEnterItem
        android:id="@+id/score_exchange_enter_msi_speed"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.alpha.alphaapp.ui.widget.LineView
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
    <com.alpha.alphaapp.ui.widget.mall.ScoreExchangeEnterItem
        android:id="@+id/score_exchange_enter_msi_super"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>



</LinearLayout>