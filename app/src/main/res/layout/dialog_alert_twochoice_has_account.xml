<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/commom_length_301"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">


        <TextView
            android:id="@+id/dialog_alert_two_ac_tv_msg_1"
            style="@style/dialog_com_tv_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/commom_margin_42"
            android:gravity="center"
            android:text="确定关联" />

        <TextView
            android:id="@+id/dialog_alert_two_ac_tv_ac"
            style="@style/dialog_com_tv_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/commom_margin_5"
            android:layout_marginTop="@dimen/commom_margin_42"
            android:gravity="center"
            android:text="13128914595"
            android:textColor="@color/common_tv_dark_red"
            android:textStyle="bold" />
    </LinearLayout>

    <TextView
        android:id="@+id/dialog_alert_two_ac_tv_msg_3"
        style="@style/dialog_com_tv_gray"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="@dimen/commom_margin_23"
        android:gravity="center"
        android:text="该帐号么?" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="@dimen/commom_margin_16"
        android:layout_marginRight="@dimen/commom_margin_16"
        android:background="@color/common_bound_line_gray" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="@dimen/commom_margin_5"
        android:orientation="horizontal">

        <Button
            android:id="@+id/dialog_alert_two_ac_btn_insure"
            style="@style/btn_com_red"
            android:layout_width="@dimen/commom_length_125"
            android:layout_marginLeft="@dimen/commom_margin_16"
            android:gravity="center"
            android:text="@string/insure" />

        <Button
            android:id="@+id/dialog_alert_two_ac_btn_cancel"
            style="@style/btn_com_gray"
            android:layout_width="@dimen/commom_length_125"
            android:layout_marginLeft="@dimen/commom_margin_20"
            android:gravity="center"
            android:text="@string/cancel" />
    </LinearLayout>

</LinearLayout>